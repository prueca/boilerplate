{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for project development",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon --ignore ./src/public -r @babel/register ./src/server.js",
    "build": "rm -rf ./dist && babel ./src -d ./dist --copy-files",
    "start": "npm run build && cp .env.example .env && node ./dist/server.js",
    "rm:css": "rm -rf ./src/public/css/*.css",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "author": "Peter John R. Rueca",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/register": "^7.10.3",
    "@babel/runtime": "^7.10.3",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dateformat": "^3.0.3",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "http-errors": "^1.8.0",
    "http-status-codes": "^1.4.0",
    "jquery": "^3.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "node-sass-middleware": "^0.11.0",
    "nodemailer": "^6.4.10",
    "pug": "^3.0.0",
    "sequelize": "^6.2.3",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.3.1"
  }
}
