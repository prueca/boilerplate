{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "Setup boilerplate for project",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon --ignore ./src/public --exec babel-node ./src/server.js",
    "build": "rm -rf ./dist && babel ./src -d ./dist --copy-files",
    "start": "npm run build && cp .env.example .env && node ./dist/server.js",
    "rm:css": "rm -rf ./src/public/css/*.css",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "author": "Peter John R. Rueca",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/runtime": "^7.6.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dateformat": "^3.0.3",
    "debug": "^4.1.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "http-errors": "^1.7.3",
    "http-status-codes": "^1.3.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "mysql2": "^1.7.0",
    "node-sass-middleware": "^0.11.0",
    "pug": "^2.0.4",
    "sequelize": "^5.19.6",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.1"
  }
}
